{
  "name": "prbrain-action",
  "version": "1.0.0",
  "description": "AI-powered GitHub Action that analyzes PRs for intent, AI detection, deduplication, and quality scoring",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx @vercel/ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/ test/ --ext .ts",
    "lint:fix": "eslint src/ test/ --ext .ts --fix",
    "package": "npm run build && npm run type-check",
    "all": "npm run lint && npm run type-check && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/prbrain.git"
  },
  "keywords": [
    "github",
    "actions",
    "pull-request",
    "ai",
    "code-review",
    "automation",
    "typescript"
  ],
  "author": "PRBrain Team",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "openai": "^4.28.0",
    "zod": "^3.22.4",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}